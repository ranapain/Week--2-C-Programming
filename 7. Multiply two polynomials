#include <stdio.h>

int main() {
    int deg1, deg2;
    printf("Enter degree of first polynomial: ");
    scanf("%d", &deg1);
    printf("Enter degree of second polynomial: ");
    scanf("%d", &deg2);

    int poly1[20] = {0}, poly2[20] = {0}, prod[40] = {0};

    printf("Enter coefficients of first polynomial:\n");
    for (int i = deg1; i >= 0; i--) scanf("%d", &poly1[i]);

    printf("Enter coefficients of second polynomial:\n");
    for (int i = deg2; i >= 0; i--) scanf("%d", &poly2[i]);

    for (int i = 0; i <= deg1; i++)
        for (int j = 0; j <= deg2; j++)
            prod[i + j] += poly1[i] * poly2[j];

    printf("Resultant Polynomial: ");
    for (int i = deg1 + deg2; i >= 0; i--)
        if (prod[i] != 0) printf("%dx^%d ", prod[i], i);

    return 0;
}
