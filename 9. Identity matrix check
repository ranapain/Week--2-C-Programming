#include <stdio.h>

int main() {
    int n, flag = 1;
    printf("Enter size of square matrix: ");
    scanf("%d", &n);

    int a[10][10];
    printf("Enter matrix elements:\n");
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &a[i][j]);

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            if (i == j && a[i][j] != 1) flag = 0;
            if (i != j && a[i][j] != 0) flag = 0;
        }
    }

    if (flag)
        printf("Matrix is Identity\n");
    else
        printf("Matrix is NOT Identity\n");

    return 0;
}
